<Window x:Class="DataHandler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataHandler"
        mc:Ignorable="d"
        Title="MainWindow" Height="632.6889" Width="808.08">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="主界面 界面A.png"/>
        </Grid.Background>
        <Button Margin="70,79,573,451" BorderBrush="{x:Null}" Content="" FontFamily="Adobe Heiti Std R" FontSize="29.333" Click="To_InputPage" Width="157.08" Height="71.6889">
            <Button.Background>
                <ImageBrush ImageSource="输入数据（边框）.png"/>
            </Button.Background>
        </Button>
        <Button Margin="321,28,339,505" BorderBrush="{x:Null}" Content="" FontFamily="Adobe Heiti Std R" FontSize="26.667" Click="GetAve" Width="140.08" Height="68.6889">
            <Button.Background>
                <ImageBrush ImageSource="求平均值.png"/>
            </Button.Background>
        </Button>
        <Button Margin="321,111,339,422" BorderBrush="{x:Null}" Content="" FontFamily="Adobe Heiti Std R" FontSize="26.667" Click="GetSigma" Width="140.08" Height="68.6889">
            <Button.Background>
                <ImageBrush ImageSource="求标准差.png"/>
            </Button.Background>
        </Button>
        <Button Margin="80,468,564,56" BorderBrush="{x:Null}" FontFamily="Adobe Heiti Std R" Content="" FontSize="40" Click="Clear" Width="156.08" Height="77.6889">
            <Button.Background>
                <ImageBrush ImageSource="清零.png"/>
            </Button.Background>
        </Button>
        <TextBlock x:Name="Out_Ave" Margin="500,33,37,502" FontSize="26.667" FontFamily="Adobe Heiti Std R" TextAlignment="Center" Width="263.08" Height="66.6889">
